{
  "manifest_version": 3,
  "name": "Eazi Calculator",
  "version": "1.0",
  "description": "A simple and easy-to-use calculator for basic arithmetic operations",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Eazi Calculator"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Open Eazi Calculator"
    }
  },
  "permissions": []
}